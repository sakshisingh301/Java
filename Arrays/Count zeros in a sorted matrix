  
  
  class GfG
{
    /*you are required to complete this method*/
    int countZeros(int A[][], int n)
    {
         int count=0;
       for(int i=0;i<n;i++)
       {
           for(int j=0;j<n;j++)
           {
               if(A[i][j]==0)
               count++;
           }
       }
       return count;
    }
}
