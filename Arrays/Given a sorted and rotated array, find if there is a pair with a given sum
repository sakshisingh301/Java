
import java.util.*;
public class Main
{
    
	public static void main(String[] args) {
		int [] a={1,2,3,4,5,6};
		int sum=5;
		boolean check=false;
		for(int i=0;i<a.length;i++)
		{
		    for(int j=i+1;j<a.length;j++)
		    {
		        if(a[i]+a[j]==sum)
		        check=true;
		    }
		}
		System.out.println(check);
	}
}
