code


public class Main
{
	public static void main(String[] args) {
		int [] a={1,2,3,4};
		int temp=a[0]; // storing first element of the array in a temp variable 
		for(int i=0;i<a.length-1;i++)
		{
		    a[i]=a[i+1]; //shifting element by one 
		}
	a[a.length-1]=temp; // finally updating last element as first element 
	for(int x:a) // printing the array
	{
	    System.out.println(x);
	}
		   
	}
		
}
