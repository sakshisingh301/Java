
import java.util.*;
public class Main
{
	public static void main(String[] args) {
		int [][] a={{1,1,1},{2,2,2},{3,3,3}};
		int [][] b={{1,1,1},{2,2,2},{3,3,3}};
		int sum=0;
		ArrayList<Integer> obj=new ArrayList<>();
		for(int i=0;i<a.length;i++)
		{
		    for(int j=0;j<b.length;j++)
		    {
		        sum=a[i][j]+b[i][j];
		        obj.add(sum);
		    }
		}
		for(int x:obj)
		{
		    System.out.println(x);
		}
	}
}
