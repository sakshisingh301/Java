
import java.util.*;
public class Main
{
	public static void main(String[] args) {
	String s="geeks";
	char [] c=s.toCharArray();
	Arrays.sort(c);
	
	int start=0;
	int end=c.length-1;
	while(start<=end)
	{
	    char temp=c[start];
	    c[start]=c[end];
	    c[end]=temp;
	    start++;
	    end--;
	}
System.out.println(new String(c));
	}
}


