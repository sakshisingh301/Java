class Solution {
    public boolean isPalindrome(int x) {
        if(x<0)
        return false;
        int temp=x;
        int sum=0;
        int r=0;
        while(x>0)
        {
         r=x%10;
         x=x/10;
         sum=sum*10+r;
        }
       
        System.out.println(sum);
         System.out.println(temp);
        
        if(temp==sum)
        {
        return true;    
        }
            
        return false;
    }
}
