
 
import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    int [] a={15,2,45,12,7};
	    int temp=-1;
	    for(int i=0;i<a.length;i++)
	    {
	        if(a[i]==i+1)
	        {
	            temp=a[i];
	            break;
	        }
	    }
	
	System.out.println(temp);
	}
}
